<%- include ('../../partials/header.ejs') %>
<%- include ('../../partials/navbar.ejs') %>

<div class="container">
    <hr>
    <h2>Usuários</h2>
    <a class="btn btn-warning" href="/admin/users/create">Novo usuário</a>
    <hr>
    <table class="table table-bordered">
        <tr>
            <th>ID</th>
            <th>Email</th>
        </tr>
        <tr>
            <% users.forEach(user => { %>
                <td><%= user.id %></td>
                <td><%= user.email %></td>
            <% }) %>
        </tr>
    </table>
</div>

<%- include ('../../partials/footer.ejs') %>